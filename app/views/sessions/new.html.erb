  <h2>LOG IN</h2>

  <%= form_tag(sessions_path) do %>
    <%= text_field_tag :email, nil, placeholder: "Login with ur email" %>
    <%= text_field_tag :password, nil, placeholder: "enter ur password" %>
    <%= submit_tag "submit" %>
  <% end %><br></br>

  <h3>Or Login with Github</h3>
  <%= link_to "login with github", '/auth/github' %>
