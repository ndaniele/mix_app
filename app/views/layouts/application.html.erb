<!DOCTYPE html>
<html>
  <head>
    <title>Mix</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

  <% @user = current_user %>
  <%= render 'layouts/navbar', :locals => {:user => @user} %>
  <%= render 'layouts/messages' %>

    <%= yield %>

<script src="/assets/javascripts/mix_app.js"></script>
  </body>

</html>
